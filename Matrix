row=int(input("enter the number of row"))
column=int(input("enter the number of columns"))
matrix=[]
print("enter the entries row wise:")
for i in range(row):
    a=[]
    for j in range(column):
        a.append(int(input()))
    matrix.append(a)
for i in range(row):
    for j in range(column):
            print(matrix[i][j],end=' ')
    print()
                 
